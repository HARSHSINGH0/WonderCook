<script>
    function testasdasd() {//this may cause error
        $("#txtrecipename").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "/Home/Getrecipes/",
                    data: "{'recipe_name':'" + request.term + "'}",
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        console.log(data);
                        response($.map(data, function (item) {
                            return item;
                        }))
                    },
                });
            },
            minLength: 1
        });
    })
</script>


<input type="text" id="txtrecipename" name="recipe_name" class="searchbar searchbarfont" placeholder="   Find..." />